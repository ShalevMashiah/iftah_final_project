# =========================
# X11 / Display
# =========================
ENABLE_IMSHOW=1
DISPLAY=:0

# =========================
# Algorithm Flow Control
# =========================
ALGO_QUEUE_SIZE=2
ALGO_MAX_NONE_COUNT=20
ALGO_NONE_SLEEP_SEC=0.05
ALGO_RENDER_SLEEP_SEC=0.05
ALGO_RENDER_FPS=30
ALGO_WINDOW_PREFIX=Camera

# =========================
# JPEG Output Settings
# =========================
ALGO_OUTPUT_DIR=/app/logs
ALGO_SAVE_JPEG=1
ALGO_JPEG_QUALITY=85
ALGO_JPEG_NAME_PATTERN=stream_{index}.jpg

# =========================
# Logging
# =========================
ALGO_LOG_FILE_PATH=/app/logs/algorithm.log
VIDEO_MANAGER_LOG_FILE_PATH=/app/logs/video_manager.log

# =========================
# Video Manager
# =========================
VIDEO_SOURCE=/app/videos/video1.mp4
VIDEO_WIDTH=640
VIDEO_HEIGHT=480
VIDEO_SHM_PATH=/dev/shm/video0
