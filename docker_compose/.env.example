# Video Configuration
VIDEO_PATH=/videos/video1.mp4
SHARED_MEMORY_NAME=video_frames_shm
VIDEO_WIDTH=1920
VIDEO_HEIGHT=1080
FPS=24

# Algorithm Service Configuration
CONNECTION_MAX_RETRIES=30
CONNECTION_RETRY_DELAY=1
LOG_INTERVAL=1.0
PROCESSING_DELAY=0.001

# Detection Configuration
DETECTION_ENABLED=true
DETECTION_TYPE=face
DETECTION_CONFIDENCE=0.5
MODEL_PATH=/models

# GStreamer Configuration (optional)
# Example: x264enc ! rtph264pay ! udpsink host=localhost port=5000
GSTREAMER_OUTPUT=

# Output Configuration
OUTPUT_VIDEO_ENABLED=true
OUTPUT_VIDEO_PATH=/output/detected_video.mp4

# Video Manager Service Configuration
LOG_FRAME_INTERVAL=100

# Logging
LOG_FILE_PATH=/app/logs/service.log
